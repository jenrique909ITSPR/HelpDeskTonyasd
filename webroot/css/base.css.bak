html,
body {
    height: 100%
}
html {
    box-sizing: border-box
}
*,
*:before,
*:after {
    -webkit-box-sizing: inherit;
    -moz-box-sizing: inherit;
    box-sizing: inherit
}
html,
body {
    font-size: 12px
}
body {
    background: #fff;
    color: #222;
    cursor: auto;
    font-family: "Helvetica Neue", Helvetica, Roboto, Arial, sans-serif;
    font-style: normal;
    font-weight: normal;
    line-height: 1.5;
    margin: 0;
    padding: 0;
    position: relative
}
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
form,
p,
blockquote,
th,
td {
    margin: 0;
    padding: 0
}
h1,
h2,
h3,
h4,
h5,
h6 {
    color: #222;
    font-family: "Helvetica Neue", Helvetica, Roboto, Arial, sans-serif;
    font-style: normal;
    font-weight: normal;
    line-height: 1.4;
    margin-bottom: 0.5rem;
    margin-top: 0.2rem;
    text-rendering: optimizeLegibility
}
a {
    color: #337AB7;
}
a:hover {
    cursor: pointer
}
img {
    max-width: 100%;
    height: auto
}
img {
    -ms-interpolation-mode: bicubic
}
.fixed {
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 99;
    left: 0
}
.left {
    float: left !important
}
.right {
    float: right !important
}
.clearfix:before,
.clearfix:after {
    content: " ";
    display: table
}
.clearfix:after {
    clear: both
}
.hide {
    display: none
}
.invisible {
    visibility: hidden
}
.antialiased {
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale
}
img {
    display: inline-block;
    vertical-align: middle
}

.top-bar {
    background: #E7E7E7;
    height: 2.8125rem;
    line-height: 2.8125rem;
    margin-bottom: 0;
    overflow: hidden;
    /*position: relative*/
}

.subtop-bar {
    background: #EEE;
    border-bottom: 1px solid #E7E7E7;
    height: 2.8125rem;
    line-height: 2.8125rem;
    margin-bottom: 0;
    overflow: hidden;
    position: relative
}
.subtop-bar ul {
    list-style: none;
    margin-bottom: 0;
}
.subtop-bar .title-area {
    margin: 0;
    position: relative
}
.subtop-bar .name {
    font-size: 16px;
    height: 2.8125rem;
    margin: 0
}
.subtop-bar .name h1 {
    font-size: 1.0625rem;
    line-height: 2.8125rem;
    margin: 0
}
.subtop-bar .name h1 a {
    color: #404040;
    display: block;
    font-weight: normal;
    padding: 0 0.9375rem;
    width: 75%
}

