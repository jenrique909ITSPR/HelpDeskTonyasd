    box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.3), 0 1px 1px rgba(0, 0, 0, 0.2);
    -webkit-user-select: none;
    user-select: none;
}
div.actions a:hover {
    background: #ededed;
    border-color: #acacac;
    text-decoration: none;
}

